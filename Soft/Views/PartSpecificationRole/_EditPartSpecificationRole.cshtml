@using HaSe.Pages.Controls
@model HaSe.Facade.Project.PartSpecificationRoleViewModel

@Html.EditItem(model => model.PartyName)
@Html.SelectItem(model => model.PartSpecificationId, "partspecification")


<div class="form-group">
    <label asp-for="Type" class="control-label"></label>
    <select asp-for="Type" class="form-control">
        <option value="">-- Select Type --</option>
        <option value="Designer">Designer</option>
        <option value="Tester">Tester</option>
        <option value="Approver">Approver</option>
    </select>
    <span asp-validation-for="Type" class="text-danger" />
</div>
